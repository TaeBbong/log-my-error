{
  "name": "my-error-logger",
  "displayName": "My Error Logger",
  "description": "Log troublesome code snippets and attach fixes without leaving VS Code.",
  "version": "0.0.1",
  "publisher": "your-name",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:myErrorLogger.log"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "myErrorLogger.log",
        "title": "Log with My-Extension"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "myErrorLogger.log",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "prepublishOnly": "npm run compile"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.4",
    "@types/vscode": "^1.101.0",
    "@vscode/vsce": "^3.6.0",
    "typescript": "^5.8.3"
  }
}
